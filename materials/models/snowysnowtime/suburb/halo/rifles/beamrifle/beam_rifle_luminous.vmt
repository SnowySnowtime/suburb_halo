"VertexLitGeneric"
{
	$basetexture	"models\snowysnowtime\suburb\halo\shared\teleporter"
	$basetexturetransform "center .5 .5 scale 8 8 rotate 90 translate 0 0"
	$color2 "[0 0 0]"

	$nocull		1
	$translucent	1

	$emissiveBlendEnabled	1
	$emissiveBlendTexture	"models\snowysnowtime\suburb\halo\shared\shield_arcs"
	$emissiveBlendBaseTexture	"models\debug\debugwhite"
	$emissiveBlendFlowTexture	"models\snowysnowtime\suburb\halo\shared\shield_flow"
	$emissiveBlendTint		"[8.5 1.5 9.5]"
	$emissiveBlendStrength 	1
	$emissiveBlendScrollVector	"[ -0.511123 0.21 ]"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedTextureVar" "$basetexture"
			"animatedTextureFrameNumVar" "$frame"
			"animatedTextureFrameRate" "30"
		}
	}
}